{
	"schemaVersion": 1,
	"id": "bloomshift",
	"version": "${version}",
	"name": "Bloomshift",
	"description": "Bloomshift adds new features to the Cherry Grove biome so it feels less empty and more alive.",
	"authors": [
		"Digi"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/bloomshift/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.digi.bloomshift.Bloomshift"
		],
		"fabric-datagen": [
			"com.digi.bloomshift.BloomshiftDataGenerator"
		],
		"client":[
			"com.digi.bloomshift.BloomshiftClient"
		]
	},
	"mixins": [
		"bloomshift.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	}
}